<!-- 글 상세보기 전 비밀번호 입력화면 -->
<!DOCTYPE html>
<script language="javascript">
        function pwCheck() {
          let pw = document.getElementById("docPass").value;
  
          if (pw != <%=prodata.docPass%>) {
            alert("암호가 다릅니다. 다시입력하세요");
            return false;
          }
          return true;
        }
      </script>
  <p>
  <div class="container">  <!-- container class 가 페이지의 컨텐츠영역을 지정 -->
   <div class="jumbotron" style="width:400px; margin:auto">

          <div class="boxbox" style='width:300px; margin:auto'>
           <form accept-charset="UTF-8" role="form" onsubmit="return pwCheck()" method="post" action="/storage/document?index=<%= docID %>">
            <h5>비밀번호를 입력해주세요!</h5> <hr>
            <div class="form-group">
                <label for="productnum">Password</label>
                <input name="docPass" id="docPass" type="password" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" class="btn btn-primary" value="확인"/>
            </div>
           </form>
          </div>

     </div>  <!-- jumbotron -->
    </div> <!-- /.container -->
